{
    "name": "ltqio/upyun-php-sdk",
    "description": "Modern fork of the official Upyun PHP SDK with PHP 8.3 support, PHPUnit 9 compatibility, and Laravel 12 support.",
    "support": {
        "source": "https://github.com/ltqio/upyun-php-sdk",
        "issues": "https://github.com/ltqio/upyun-php-sdk/issues"
    },
    "keywords": ["upyun", "php", "sdk", "laravel12", "php8.3", "guzzle7", "fork"],
    "type": "library",
    "minimum-stability": "stable",
    "homepage": "https://github.com/ltqio/upyun-php-sdk",
    "license": "MIT",
    "require": {
        "php": ">=8.3",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": { "Upyun\\": "src/Upyun/" }
    },
    "autoload-dev": {
        "psr-4": { "Upyun\\Tests\\": "tests/" }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
